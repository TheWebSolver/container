{
  "name": "thewebsolver/container",
  "description": "A simple container for Dependency Injection and Auto-wiring",
  "license": "GPL-3.0-or-later",
  "type": "library",
  "keywords": [
    "psr-11",
    "container",
    "dependency-injection",
    "auto-wiring",
    "library",
    "DI"
  ],
  "require": {
    "php": ">=8.2.0",
    "psr/container": "^2.0",
    "psr/event-dispatcher": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "TheWebSolver\\Codegarage\\Lib\\Container\\": "Src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
